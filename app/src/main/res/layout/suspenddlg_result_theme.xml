<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="fill_parent"
    android:layout_height="wrap_content"
    android:orientation="vertical"
>   <!--***each line of score result***-->
    <LinearLayout
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
    >
        <LinearLayout
             android:layout_width="0dp"
             android:layout_weight="0.12"
             android:layout_height="wrap_content"
             android:orientation="horizontal"
                android:gravity="center_vertical"
        >
            <TextView
                 android:layout_width="0dp"
                 android:layout_weight="0.5"
                 android:layout_height="wrap_content"
                android:gravity="center_vertical"
                        android:textSize="@dimen/smallfont_textsize_dialog_textview_middle"
                android:id="@+id/StartingEswn"
            /> 
            <TextView
                 android:layout_width="0dp"
                 android:layout_weight="0.7"
                 android:layout_height="wrap_content"
                android:gravity="center"
                        android:textSize="@dimen/smallfont_textsize_dialog_textview_middle"
                android:id="@+id/Order"
            /> 
        </LinearLayout>
        <LinearLayout
             android:layout_width="0dp"
             android:layout_weight="0.50"
             android:layout_height="wrap_content"
             android:orientation="horizontal"
                    android:layout_gravity="center_vertical"
                    android:gravity="center_vertical"
        >
            <TextView
                 android:layout_width="0dp"
                 android:layout_weight="0.2"
                 android:layout_height="wrap_content"
    		        android:gravity="right"
                android:id="@+id/tvBackPointReach"
            />
            <TextView
                 android:layout_width="0dp"
                 android:layout_weight="0.40"
                 android:layout_height="wrap_content"
        		    android:gravity="right"
                android:id="@+id/Score"
                        android:textSize="@dimen/smallfont_textsize_dialog_textview_middle"
                    android:background="@color/editable"
            />
            <TextView
                 android:layout_width="0dp"
                 android:layout_weight="0.2"
                 android:layout_height="wrap_content"
    		        android:gravity="right"
                android:id="@+id/TopPrize"
            />
            <TextView
                 android:layout_width="0dp"
                 android:layout_weight="0.2"
                 android:layout_height="wrap_content"
		            android:gravity="right"
                android:id="@+id/OrderPrize"
            />
        </LinearLayout>
        <LinearLayout
             android:layout_width="0dp"
             android:layout_weight="0.38"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
                        android:gravity="center_vertical"
        >
            <TextView
                 android:layout_height="wrap_content"
                 android:layout_width="fill_parent"
                android:gravity="center_vertical"
                android:layout_gravity="center_vertical"
                     android:id="@+id/memberName"
                        android:textSize="@dimen/smallfont_textsize_dialog_textview_middle"
                    android:layout_marginLeft="10dp"
            />     
             <!--   android:text="name"-->
		</LinearLayout>
	</LinearLayout><!--1st line-->

    <LinearLayout
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
    >             <!--2nd line-->

        <LinearLayout
             android:layout_width="0dp"
             android:layout_weight="0.12"
             android:layout_height="wrap_content"
             android:orientation="horizontal"
        />
        <LinearLayout
             android:layout_width="0dp"
             android:layout_weight="0.30"
             android:layout_height="wrap_content"
             android:orientation="horizontal"
                    android:layout_gravity="center_vertical"
        >
            <LinearLayout
                 android:layout_height="wrap_content"
                 android:layout_width="fill_parent"
                android:orientation="horizontal"
                        android:id="@+id/llSuspend"
                    android:gravity="center_vertical"
            >  <!--suspendDlg-->
                <LinearLayout
                     android:layout_height="wrap_content"
                     android:layout_width="0dp"
                     android:layout_weight="0.2"
                    android:orientation="horizontal"
                        android:id="@+id/llSuspendPenalty"
                />
               <!--         android:id="@+id/llSuspendPenalty"-->
                <LinearLayout
                     android:layout_height="wrap_content"
                     android:layout_width="0dp"
                     android:layout_weight="0.4"
                    android:orientation="horizontal"
                    android:gravity="center_vertical"
                >
                   <!--     android:id="@+id/llSuspendPenalty" -->
                    <TextView
                         android:layout_width="fill_parent"
                         android:layout_height="wrap_content"
                        android:gravity="right"
                        android:id="@+id/tvSuspendPenalty"
                    />
                </LinearLayout>
            </LinearLayout>

        </LinearLayout>
        <LinearLayout
             android:layout_height="wrap_content"
             android:layout_width="0dp"
             android:layout_weight="0.2"
        	android:orientation="horizontal"
        >
            <TextView
                 android:layout_height="wrap_content"
                 android:layout_width="fill_parent"
                android:gravity="center_vertical|right"
                android:id="@+id/finalScore"
                    android:background="@color/editable"
                        android:textSize="@dimen/smallfont_textsize_dialog_textview_middle"
            />
        </LinearLayout>
        <LinearLayout
             android:layout_height="wrap_content"
             android:layout_width="0dp"
             android:layout_weight="0.38"
            android:orientation="horizontal"
                android:gravity="center_vertical"
        >
            <LinearLayout
                 android:layout_height="wrap_content"
                 android:layout_width="0dp"
                     android:layout_weight="0.5"
                android:orientation="horizontal"
                    android:gravity="center_vertical"
                    android:id="@+id/llForBotCharge"
            >
                <TextView
                     android:layout_height="wrap_content"
                     android:layout_width="fill_parent"
                    android:gravity="center_vertical|right"
                    android:id="@+id/tvForBot"
                />
            </LinearLayout>
            <LinearLayout
                 android:layout_height="wrap_content"
                 android:layout_width="0dp"
                 android:layout_weight="0.5"
                android:orientation="horizontal"
                        android:gravity="center_vertical"
            >
                <TextView
                     android:layout_height="wrap_content"
                     android:layout_width="fill_parent"
                    android:gravity="center_vertical|right"
                    android:background="@color/editable"
                    android:id="@+id/finalPoint"
                    android:layout_marginRight="2dp"
                        android:textSize="@dimen/smallfont_textsize_dialog_textview_middle"
                />
        	</LinearLayout>
        </LinearLayout>
    </LinearLayout><!--2nd line-->
</LinearLayout>

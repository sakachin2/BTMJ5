<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="fill_parent"
    android:layout_height="wrap_content"
    android:orientation="vertical"
>
    <LinearLayout
         android:layout_width="fill_parent"
         android:layout_height="wrap_content"
        android:orientation="horizontal"
                    android:gravity="center_vertical"
    >
        <TextView
            android:text="@string/Label_S2P"
            android:layout_height="wrap_content"
            android:layout_width="wrap_content"
                android:gravity="center_vertical"
                android:layout_gravity="center_vertical"
                     android:textSize="@dimen/smallfont_textsize_dialog_textview_middle"
        />
        <!--    android:textSize="@dimen/textview_middle"-->
    </LinearLayout>
    <LinearLayout
         android:layout_width="fill_parent"
         android:layout_height="wrap_content"
        android:orientation="horizontal"
                    android:gravity="center_vertical"
    >
        <CheckBox
            android:id="@+id/cbRankMUp"
            android:text="@string/Label_RankMUp"
            android:layout_height="wrap_content"
            android:layout_width="wrap_content"
        />
        <TextView
            android:text="@string/Desc_RankMUp"
            android:layout_height="wrap_content"
            android:layout_width="wrap_content"
                android:gravity="center_vertical"
                android:layout_gravity="center_vertical"
        />
    </LinearLayout>
    <LinearLayout
         android:layout_width="fill_parent"
         android:layout_height="wrap_content"
        android:orientation="vertical"
                    android:gravity="center_vertical"
    >
        <LinearLayout
             android:layout_width="wrap_content"
             android:layout_height="wrap_content"
            android:orientation="horizontal"
        >
            <TextView
                android:text="@string/Label_S2PLast"
                android:layout_height="wrap_content"
                android:layout_width="wrap_content"
                    android:gravity="center_vertical"
                    android:layout_gravity="center_vertical"
                     android:textSize="@dimen/smallfont_textsize_dialog_textview_middle"
            />
        </LinearLayout>
        <LinearLayout
             android:layout_width="wrap_content"
             android:layout_height="wrap_content"
            android:orientation="horizontal"
        >
            <RadioButton
                android:id="@+id/rbS2P_No"
                android:text="@string/Label_S2P_No"
                android:layout_height="wrap_content"
                android:layout_width="wrap_content"
            />
            <RadioButton
                android:id="@+id/rbS2P_Floor"
                android:text="@string/Label_S2P_Floor"
                android:layout_height="wrap_content"
                android:layout_width="wrap_content"
            />
            <RadioButton
                android:id="@+id/rbS2P_Ceiling"
                android:text="@string/Label_S2P_Ceiling"
                android:layout_height="wrap_content"
                android:layout_width="wrap_content"
            />
            <RadioButton
                android:id="@+id/rbS2P_45"
                android:text="@string/Label_S2P_45"
                android:layout_height="wrap_content"
                android:layout_width="wrap_content"
            />
        <!--/RadioGroup-->
        </LinearLayout>
    </LinearLayout>    <!--line1-->
    <LinearLayout
         android:layout_width="fill_parent"
         android:layout_height="wrap_content"
        android:orientation="horizontal"
    >
        <LinearLayout
             android:layout_width="wrap_content"
             android:layout_height="wrap_content"
            android:orientation="horizontal"
        >
            <RadioButton
                android:id="@+id/rbS2P_56"
                android:text="@string/Label_S2P_56"
                android:layout_height="wrap_content"
                android:layout_width="wrap_content"
            />
            <RadioButton
                android:id="@+id/rbS2P_PM"
                android:text="@string/Label_S2P_PM"
                android:layout_height="wrap_content"
                android:layout_width="wrap_content"
            />
        </LinearLayout>
    </LinearLayout>    <!--line2-->
    <LinearLayout
         android:layout_width="wrap_content"
         android:layout_height="wrap_content"
        android:orientation="horizontal"
    >
            <TextView
                android:text="@string/Label_S2P_Zero"
                android:layout_height="wrap_content"
                android:layout_width="wrap_content"
            />
            <!--    android:id="@+id/rbS2P_Floor" -->
    </LinearLayout>    <!--line3-->
</LinearLayout>

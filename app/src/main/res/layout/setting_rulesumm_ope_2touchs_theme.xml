    <LinearLayout
         android:layout_width="wrap_content"
         android:layout_height="wrap_content"
        android:orientation="horizontal"
    >
        <TextView
            android:layout_height="wrap_content"
            android:layout_width="wrap_content"
                android:text="@string/Label_YakuFix1"
                     android:textSize="@dimen/smallfont_textsize_dialog_textview_middle"
        />
    </LinearLayout>
    <LinearLayout
         android:layout_width="wrap_content"
         android:layout_height="wrap_content"
        android:orientation="horizontal"
    >
        <CheckBox
            android:id="@+id/cbYakuFix1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/Label_YakuFix1Yes"
        />
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/Desc_YakuFix1"
        />
    </LinearLayout>
    <LinearLayout
         android:layout_width="wrap_content"
         android:layout_height="wrap_content"
        android:orientation="vertical"
    >
        <LinearLayout
             android:layout_width="wrap_content"
             android:layout_height="wrap_content"
            android:orientation="horizontal"
        >
            <TextView
                android:layout_height="wrap_content"
                android:layout_width="wrap_content"
                    android:text="@string/Label_ChkMultiWait"
                     android:textSize="@dimen/smallfont_textsize_dialog_textview_middle"
            />
        </LinearLayout>
        <LinearLayout
             android:layout_width="wrap_content"
             android:layout_height="wrap_content"
            android:orientation="horizontal"
        >
            <CheckBox
                android:id="@+id/cbChkMultiWait"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/Label_ChkMultiWaitYes"
            />
	    </LinearLayout>
<!--
        <TextView
            android:layout_width="wrap_content"

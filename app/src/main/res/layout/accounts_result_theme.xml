<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="fill_parent"
    android:layout_height="wrap_content"
    android:orientation="vertical"
>   <!--***each line of score result***-->
    <LinearLayout
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
                android:gravity="center_vertical"
    >
        <LinearLayout
             android:layout_width="0dp"
             android:layout_weight="0.10"
             android:layout_height="wrap_content"
             android:orientation="horizontal"
                android:gravity="center_vertical"
        >
            <TextView
                 android:layout_width="0dp"
                 android:layout_weight="0.5"
                 android:layout_height="wrap_content"
                android:gravity="center_vertical"
                        android:textSize="@dimen/smallfont_textsize_dialog_textview_middle"
                android:id="@+id/StartingEswn"
                android:maxLines="1"
            /> 
            <TextView
                 android:layout_width="0dp"
                 android:layout_weight="0.5"
                 android:layout_height="wrap_content"
                android:gravity="center"
                        android:textSize="@dimen/smallfont_textsize_dialog_textview_middle"
                android:id="@+id/Order"
                android:maxLines="1"
            /> 
        </LinearLayout>
        <LinearLayout
             android:layout_width="0dp"
             android:layout_weight="0.52"
             android:layout_height="wrap_content"
             android:orientation="horizontal"
                    android:gravity="center_vertical"
        >
            <LinearLayout
                 android:layout_width="0dp"
                 android:layout_weight="0.48"
                 android:layout_height="wrap_content"
                 android:orientation="horizontal"
                    android:gravity="center_vertical"
            >
                <TextView
                     android:layout_width="0dp"
                     android:layout_weight="0.60"
                     android:layout_height="wrap_content"
                android:gravity="right|center_vertical"
                    android:id="@+id/Score"
                    android:background="@color/editable"
                android:maxLines="1"
                />
                <TextView
                     android:layout_width="0dp"
                     android:layout_weight="0.40"
                     android:layout_height="wrap_content"
                android:gravity="right"
                    android:id="@+id/TopPrize"
                android:maxLines="1"
                />
            </LinearLayout>
            <LinearLayout
                 android:layout_width="0dp"
                 android:layout_weight="0.52"
                 android:layout_height="wrap_content"
                 android:orientation="horizontal"
            >
                <TextView
                     android:layout_width="0dp"
                     android:layout_weight="0.40"
                     android:layout_height="wrap_content"
                android:gravity="right"
                    android:id="@+id/OrderPrize"
                android:maxLines="1"
                />
                <TextView
                     android:layout_width="0dp"
                     android:layout_weight="0.60"
                     android:layout_height="wrap_content"
                android:gravity="right"
                    android:id="@+id/Total"
                android:maxLines="1"
                />
            </LinearLayout>
        </LinearLayout>
        <LinearLayout
             android:layout_width="0dp"
             android:layout_weight="0.38"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
                        android:gravity="center_vertical"
        >
            <TextView
                 android:layout_height="wrap_content"
                 android:layout_width="fill_parent"
                android:gravity="center_vertical"
                android:layout_gravity="center_vertical"
                     android:id="@+id/memberName"
                        android:textSize="@dimen/smallfont_textsize_dialog_textview_middle"
                    android:layout_marginLeft="5dp"
                android:maxLines="1"
            />     
            <!--    android:text="name"-->
		</LinearLayout>
	</LinearLayout><!--1st line-->

    <LinearLayout
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
    >             <!--2nd line-->

        <LinearLayout
             android:layout_width="0dp"
             android:layout_weight="0.42"
             android:layout_height="wrap_content"
             android:orientation="horizontal"
                    android:layout_gravity="center_vertical"
        >
            <LinearLayout
                 android:layout_height="wrap_content"
                 android:layout_width="fill_parent"
                android:orientation="horizontal"
                    android:gravity="center_vertical"
                        android:id="@+id/llMinusPay"
            >  <!--accountsDlg-->
                <LinearLayout
                     android:layout_height="wrap_content"
                     android:layout_width="0dp"
                     android:layout_weight="0.4"
                    android:orientation="horizontal"
                        android:gravity="center_vertical"
                        android:id="@+id/llMinusPrize"
                >
                    <TextView
                         android:layout_width="fill_parent"
                         android:layout_height="wrap_content"
                        android:gravity="center_vertical|right"
                        android:id="@+id/minusPrize"
                android:maxLines="1"
                    />
                </LinearLayout>
                <LinearLayout
                     android:layout_height="wrap_content"
                     android:layout_width="0dp"
                     android:layout_weight="0.3"
                    android:orientation="horizontal"
                        android:gravity="center_vertical"
                        android:id="@+id/llMinusCharge"
                >
                    <TextView
                         android:layout_width="fill_parent"
                         android:layout_height="wrap_content"
                        android:gravity="center_vertical|right"
                        android:id="@+id/minusCharge"
                android:maxLines="1"
                    />
                </LinearLayout>
            </LinearLayout>

        </LinearLayout>
        <LinearLayout
             android:layout_height="wrap_content"
             android:layout_width="0dp"
             android:layout_weight="0.2"
        	android:orientation="horizontal"
        >
            <TextView
                 android:layout_height="wrap_content"
                 android:layout_width="fill_parent"
                android:gravity="center_vertical|right"
                android:id="@+id/finalScore"
                    android:background="@color/editable"
                        android:textSize="@dimen/smallfont_textsize_dialog_textview_middle"
                android:maxLines="1"
            />
        </LinearLayout>
        <LinearLayout
             android:layout_height="wrap_content"
             android:layout_width="0dp"
             android:layout_weight="0.38"
            android:orientation="horizontal"
                android:gravity="center_vertical"
        >
            <LinearLayout
                 android:layout_height="wrap_content"
                 android:layout_width="0dp"
                     android:layout_weight="0.5"
                android:orientation="horizontal"
                    android:gravity="center_vertical"
                    android:id="@+id/llForBotCharge"
            >
                <TextView
                     android:layout_height="wrap_content"
                     android:layout_width="fill_parent"
                    android:gravity="center_vertical|right"
                    android:id="@+id/tvForBot"
                android:maxLines="1"
                />
            </LinearLayout>
            <LinearLayout
                 android:layout_height="wrap_content"
                 android:layout_width="0dp"
                 android:layout_weight="0.5"
                android:orientation="horizontal"
                        android:gravity="center_vertical"
            >
                <TextView
                     android:layout_height="wrap_content"
                     android:layout_width="fill_parent"
                    android:gravity="center_vertical|right"
                    android:background="@color/editable"
                    android:id="@+id/finalPoint"
                    android:layout_marginRight="5dp"
                        android:textSize="@dimen/smallfont_textsize_dialog_textview_middle"
                android:maxLines="1"
                />
        	</LinearLayout>
        </LinearLayout>
    </LinearLayout><!--2nd line-->
</LinearLayout>
